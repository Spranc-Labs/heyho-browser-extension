{
  "manifest_version": 3,
  "name": "HeyHo Extension - Analyze browser journey",
  "version": "1.0.0",
  "description": "A browser extension to intelligently track browsing habits and generate insights to help users understand and manage their digital activity.",
  "background": {
    "scripts": [
      "browser-polyfill.js", 
      "src/aggregation/utils.js",
      "src/aggregation/models.js",
      "src/aggregation/storage.js",
      "src/aggregation/processor.js",
      "src/background/storage.js", 
      "src/background/config.js",
      "src/background/triage.js",
      "src/background/events.js",
      "src/background/aggregator.js",
      "src/background/heartbeat.js",
      "src/background/debug.js",
      "src/background/listeners.js", 
      "src/background/cleanup.js",
      "src/background/init.js",
      "test-aggregation-fix.js",
      "background.js"
    ]
  },
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_title": "HeyHo Extension Debug Panel"
  },
  "permissions": [
    "tabs",
    "storage",
    "idle",
    "alarms"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "heyho@bext.com",
      "strict_min_version": "109.0"
    }
  }
}